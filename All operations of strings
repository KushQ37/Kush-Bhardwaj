#include<stdio.h>

#include<string.h>

int main()
{

char user[20];

printf("Enter the string:\n");

scanf("%s",&user);

printf("\n The length of this string: %d", strlen(user));

char name[20];

printf("\n The string copy is: %s",strcpy(name,user)); 
printf("\n The string cont is: %s",strcat(name,user));

printf("\n The string comp is: %d",strcmp(name,user)); 
if(strcmp(user,name)==0)

printf("\n The Strings are equal");

} 
else
{
printf("\n Not Equal");

} 
return 0;

}
